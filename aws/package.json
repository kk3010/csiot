{
  "name": "aws-emulator-bridge",
  "version": "1.0.0",
  "description": "A bridge between a gpio emulator running a REST API and AWS IoT",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "nodemon src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --ext .ts ./src",
    "format": "prettier --write './**/*.+(js|ts|json)'",
    "prepare": "cd .. && husky install aws/.husky",
    "doc": "typedoc src"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-iot-device-sdk-v2": "^1.4.7",
    "dotenv": "^10.0.0",
    "node-fetch": "^2.6.1",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.1",
    "@types/node-fetch": "^2.5.10",
    "@types/yargs": "^16.0.2",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  }
}
